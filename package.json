  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --port 3006",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --fix",
    "format": "prettier --write \"./**/*.{ts,js,json,vue,css,scss,sass}\"",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "husky"
